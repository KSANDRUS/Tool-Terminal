echo "\e[0;1m


Tải dữ liệu vui lòng chờ...
"
Get=`curl -#G 'http://www.mediafire.com/file/zsx1mvn7f0l30le/Apktool/file' | grep http://download | cut -d '"' -f 2`
curl -L# "$Get" -o /sdcard/Tool-VH/.tmp/Apktool
tar -jxf /sdcard/Tool-VH/.tmp/Apktool -C /sdcard/Tool-VH/.tmp
cp -af /sdcard/Tool-VH/.tmp/data/* /data
rm -fr /sdcard/Tool-VH/.tmp/*
chmod -R 777 /data/data/per.pqy.openjdk
chmod -R 777 /data/local/Tool-Apk

curl -L# https://github.com/kakathic/Tool-Tool/raw/master/File/zip.7z -o /sdcard/TT3
cd /storage/emulated/0/Tool-VH/.tmp
/data/local/Tool-Apk/apktool/openjdk/bin/7za x -y /sdcard/TT3
cp -rf /sdcard/Tool-VH/.tmp/zip/* /data/local/Tool-Apk/apktool/openjdk/bin
chmod -R 777 /data/local/Tool-Apk/apktool/openjdk/bin/zip
chmod -R 777 /data/local/Tool-Apk/apktool/openjdk/bin/zipsigner-3.0-dexed.jar
chmod -R 777 /data/local/Tool-Apk/apktool/openjdk/bin/zipsigner
chmod -R 777 /data/local/Tool-Apk/apktool/openjdk/bin/zipalign
rm -fr /sdcard/Tool-VH/.tmp/zip
rm -fr /sdcard/TT3
