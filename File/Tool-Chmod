#Cấp quyền
chmod -R 755 /system/*/*.ttf
chmod -R 755 /system/*/*.otf
chmod -R 755 /system/app/*
chmod -R 755 /system/app/*/*.apk
chmod -R 755 /system/priv-app/*
chmod -R 755 /system/priv-app/*/*.apk
chmod -R 755 /system/media/*
chmod -R 755 /system/product/app/*
chmod -R 755 /system/product/app/*/*.apk
chmod -R 755 /system/product/priv-app/*
chmod -R 755 /system/product/priv-app/*/*.apk
chmod -R 755 /system/etc/precust_theme